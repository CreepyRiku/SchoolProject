<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
    <managed-bean>
        <managed-bean-name>loginBean</managed-bean-name>
        <managed-bean-class>at.htlpinkafeld.schoolproject.Beans.LoginBean</managed-bean-class>
         <managed-bean-scope>session</managed-bean-scope>
        <managed-property>
            <property-name>srv</property-name>
            <value>#{userTestService}</value>
        </managed-property>    
    </managed-bean>
    
    <managed-bean>
        <managed-bean-name>userService</managed-bean-name>
        <managed-bean-class>at.htlpinkafeld.schoolproject.Services.UserService</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    
    <managed-bean>
        <managed-bean-name>userTestService</managed-bean-name>
        <managed-bean-class>at.htlpinkafeld.schoolproject.Services.UserTestService</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>userConverter</managed-bean-name>
        <managed-bean-class>at.htlpinkafeld.schoolproject.util.UserConverter</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
        
        <managed-property>
            <property-name>srv</property-name>
            <value>#{userTestService}</value>
        </managed-property>  
    </managed-bean>
</faces-config>
